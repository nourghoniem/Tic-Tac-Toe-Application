            <Font size="38.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="28.0" layoutY="95.0" prefHeight="216.0" prefWidth="545.0">
        <columns>
          <TableColumn fx:id="username" prefWidth="273.0" text="username" />
          <TableColumn fx:id="status" minWidth="0.0" prefWidth="271.0" text="status" />
        </columns>
      </TableView>
      <Button fx:id="choose_mode_button" layoutX="420.0" layoutY="333.0" mnemonicParsing="false" onAction="#choosePlayer" prefHeight="44.0" prefWidth="143.0" text="Start Game" />
